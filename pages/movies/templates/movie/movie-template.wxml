<!--pages/movies/templates/movie/movie-template.wxml-->
<import src="../stars/stars-template.wxml" />
<template name="movieTemp">
  <view class="movie-container" data-movieId="{{movieId}}">
    <image class="movie-img" src="{{movieCoverImg}}"></image>
    <text class="movie-name">{{title}}</text>
    <template is="starsTemp" data="{{movieScore: movieScore, movieStar: movieStar}}" />
  </view>
</template>
